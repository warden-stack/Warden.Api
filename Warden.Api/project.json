{
  "title": "Warden.Api",
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true
  },
  "dependencies": {
    "Warden.Common": "1.0.*",
    "Warden.Services.Operations.Shared": "1.0.*",
    "Warden.Services.Features.Shared": "1.0.*",
    "Warden.Services.Organizations.Shared": "1.0.*",
    "Warden.Services.Spawn.Shared": "1.0.*",
    "Warden.Services.Stats.Shared": "1.0.*",
    "Warden.Services.Users.Shared": "1.0.*",
    "Warden.Services.WardenChecks.Shared": "1.0.*",
    "Autofac": "4.2.1",
    "Autofac.Extensions.DependencyInjection": "4.0.0",
    "AutoMapper": "5.2.0",
    "Exceptionless": "4.0.1922",
    "Lockbox.Client": "1.0.0",
    "Humanizer.Core": "2.1.0",
    "jose-jwt": "2.1.0",
    "Microsoft.AspNetCore.Authentication.JwtBearer": "1.1.0",
    "Microsoft.AspNetCore.Diagnostics": "1.1.0",
    "Microsoft.AspNetCore.Hosting": "1.1.0",
    "Microsoft.AspNetCore.Owin": "1.1.0",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.1.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.1.0",
    "Microsoft.AspNetCore.Cors": "1.1.0",
    "Microsoft.Extensions.Caching.Memory": "1.1.0",
    "Microsoft.Extensions.Caching.Abstractions": "1.1.0",
    "Microsoft.Extensions.Configuration.Binder": "1.1.0",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.1.0",
    "Microsoft.Extensions.Configuration.FileExtensions": "1.1.0",
    "Microsoft.Extensions.Configuration.Json": "1.1.0",
    "Microsoft.Extensions.Logging.Console": "1.1.0",
    "Microsoft.Extensions.WebEncoders": "1.1.0",
    "MongoDB.Driver": "2.4.1",
    "Nancy": "2.0.0-clinteastwood",
    "NLog.Extensions.Logging": "1.0.0-*",
    "NLog.Web.AspNetCore": "4.3.0",
    "Newtonsoft.Json": "9.0.1",
    "RawRabbit": "1.10.0",
    "RawRabbit.vNext": "1.10.0",
    "System.Linq": "4.3.0",
    "System.Reflection": "4.3.0",
    "System.Reflection.TypeExtensions": "4.3.0"
  },
  "frameworks": {
    "netcoreapp1.1": {
 			"dependencies": {
	    	"Microsoft.NETCore.App": {
					"type": "platform",
					"version": "1.1.0"
	    	}
    	}
		}
  },
  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.1.0-preview4-final"
  },
  "publishOptions": {
    "include": [
      "web.config",
      "nlog.config",
      "appsettings.json"
    ]
  },
  "scripts": {
    "postpublish": [ "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%" ]
  }
}
